{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2022"],
    "module": "Node16",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "outDir": "out",
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "target": "ES2022"
  }
}
